@model IEnumerable<CourseSite.Models.Publication>
@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Пости</h1>

    <div>
        @foreach (var publication in ViewBag.Publications)
        {
            <div class="publication" style="background-color: rgb(199, 199, 199);">
                <h3>@publication.Title</h3>
                <p>@publication.Text</p>
                <p>@publication.DateTime @publication.Author</p>
            </div>
        }
    </div>

    <p>Learn about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
</div>
